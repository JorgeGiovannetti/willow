fn myFact() {
    writeln("Hello myFact!");

    n: int = 5;
    result: int = 1;
    for(i: int -> 2..(n+1)) {
        result *= i;
    }

    writeln("myFact result: " + result);
}

fn paramTest(a: int, b: int): int {
    writeln("Param a = " + a);
    writeln("Param b = " + b);

    return 0;
}

fn main() {
    writeln("I'm in main right now!");

    myFact();
    
    x: int = paramTest(5, 3);

    writeln("x = " + x);

    writeln("I'm out of the function right now!");
}